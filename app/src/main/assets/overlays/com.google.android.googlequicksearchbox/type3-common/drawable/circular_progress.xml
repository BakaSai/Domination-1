<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/secondaryProgress">
        <shape android:shape="ring" android:useLevel="true" android:thickness="4.0dip">
            <gradient android:startColor="@*android:color/ripple_material_dark" android:endColor="@*android:color/ripple_material_dark" android:type="sweep" android:centerColor="@*android:color/ripple_material_dark" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:useLevel="true" android:thickness="4.0dip">
                <rotate android:fromDegrees="0.0" android:toDegrees="360.0" android:pivotX="50.0%" android:pivotY="50.0%" />
                <gradient android:startColor="@*android:color/white" android:endColor="@*android:color/white" android:type="sweep" android:centerColor="@*android:color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>
