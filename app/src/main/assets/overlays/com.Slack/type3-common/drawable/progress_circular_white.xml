<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@*android:color/button_material_dark" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="2.6" android:thicknessRatio="18.0" android:useLevel="true">
                <solid android:color="@*android:color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>
