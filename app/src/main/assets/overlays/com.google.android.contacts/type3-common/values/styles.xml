<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AssistantListFooter">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:paddingStart">16.0dip</item>
        <item name="android:paddingEnd">16.0dip</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="ContactsTheme.NoActionBar.LaunchScreen" parent="@*com.google.android.contacts:style/ContactsTheme.NoActionBar">
        <item name="android:windowBackground">@*com.google.android.contacts:drawable/quantum_launchscreen_contacts</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="Widget.Design.BottomSheet.Modal" parent="@*android:style/Widget">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:elevation">@*com.google.android.contacts:dimen/design_bottom_sheet_modal_elevation</item>
        <item name="*com.google.android.contacts:behavior_hideable">true</item>
        <item name="*com.google.android.contacts:behavior_peekHeight">auto</item>
        <item name="*com.google.android.contacts:behavior_skipCollapsed">false</item>
    </style>
    <style name="Widget.GoogleMaterial.FloatingActionButton" parent="@*com.google.android.contacts:style/Widget.MaterialComponents.FloatingActionButton">
        <item name="*com.google.android.contacts:backgroundTint">#ff1976d2</item>
        <item name="*com.google.android.contacts:elevation">@*com.google.android.contacts:dimen/google_fab_elevation</item>
        <item name="*com.google.android.contacts:enforceMaterialTheme">true</item>
        <item name="*com.google.android.contacts:hoveredFocusedTranslationZ">@*com.google.android.contacts:dimen/google_fab_translation_z_hovered_focused</item>
        <item name="*com.google.android.contacts:pressedTranslationZ">@*com.google.android.contacts:dimen/google_fab_translation_z_pressed</item>
        <item name="*com.google.android.contacts:rippleColor">@*android:color/ripple_material_dark</item>
        <item name="*com.google.android.contacts:tint">@*android:color/white</item>
        <item name="*com.google.android.contacts:srcCompat">@*com.google.android.contacts:drawable/quantum_gm_ic_add_vd_theme_24</item>
    </style>
</resources>
