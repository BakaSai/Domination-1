<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff1976d2" />
            <corners android:radius="8.0dip" />
            <stroke android:width="@*com.instagram.android:dimen/button_border_stroke" android:color="#ff1976d2" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@*android:color/button_material_dark" />
            <corners android:radius="8.0dip" />
            <stroke android:width="@*com.instagram.android:dimen/button_border_stroke" android:color="@*android:color/white" />
        </shape>
    </item>
</selector>
