<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HomeTheme" parent="@*com.google.android.apps.googlevoice:style/BaseTheme">
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="*com.google.android.apps.googlevoice:windowActionModeOverlay">true</item>
        <item name="*com.google.android.apps.googlevoice:actionModeStyle">@*com.google.android.apps.googlevoice:style/VoiceActionModeBar</item>
        <item name="*com.google.android.apps.googlevoice:actionModeBackground">@*com.google.android.apps.googlevoice:color/quantum_googblue</item>
        <item name="*com.google.android.apps.googlevoice:actionModeCloseDrawable">@*com.google.android.apps.googlevoice:drawable/quantum_ic_close_white_24</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.apps.googlevoice:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="InCallTheme" parent="@*com.google.android.apps.googlevoice:style/Theme.AppCompat">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
</resources>
