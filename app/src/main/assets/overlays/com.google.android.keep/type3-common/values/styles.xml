<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.DeviceDefault.Light" />

    <style name="AppBaseTheme" parent="@*android:style/Theme.DeviceDefault.Light" />

    <style name="ZoomingActivityThemeLight" parent="@*android:style/Theme.Holo">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowActionBar">true</item>
    </style>

    <style name="KeepPhotoViewerTheme" parent="@*com.google.android.keep:style/Theme.AppCompat">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.keep:popupTheme">@*com.google.android.keep:style/ThemeOverlay.AppCompat</item>
    </style>



    <style name="ShareActivityShareeEmail">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="ShareActivityInvalidShareeEmail">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>


    <style name="DrawerLandingPageItemTextActiveStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="EditorConflictResolutionTimestampStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffbbbbbb</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingTop">32.0dip</item>
        <item name="android:paddingRight">12.0dip</item>
    </style>

    <style name="EditorConflictAlertOverlayStyle">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">#b3000000</item>
        <item name="android:padding">12.0dip</item>
    </style>
    <style name="EditorConflictAlertTextStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">4.0dip</item>
    </style>

 
    <style name="WebLinkAnnotationButton">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffbbbbbb</item>
        <item name="android:gravity">left</item>
        <item name="android:background">?*android:selectableItemBackgroundBorderless</item>
        <item name="android:paddingLeft">18.0dip</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingRight">18.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">4.0dip</item>
        <item name="android:layout_below">@*com.google.android.keep:id/annotations_list</item>
    </style>



    <style name="RecipientEditTextViewBase">
        <item name="android:textAppearance">?*android:textAppearanceMedium</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@*com.google.android.keep:dimen/chip_height</item>
        <item name="android:popupBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:inputType">textEmailAddress|textMultiLine</item>
        <item name="android:textDirection">locale</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="ReminderDialogSpinner" parent="@*android:style/Widget.Holo.Spinner">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>
