<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBar" parent="@*android:style/Widget.Material.ActionBar.Solid" />
    <style name="ActionBar.Preferences" parent="@*com.google.android.calendar:style/ActionBar">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:displayOptions">homeAsUp|showTitle</item>
    </style>

    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Subtitle.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Subtitle" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title" />

    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText.Inverse" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />

    <style name="BottomSheetActionButtonsThemeBase" parent="@*android:style/Widget.Material.Button.Borderless" />

    <style name="BundleActionBarV21">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:elevation">@*com.google.android.calendar:dimen/actionbar_elevation</item>
    </style>
    <style name="PreferencesThemeBase" parent="@*android:style/Theme.DeviceDefault.Light" />
    <style name="EventInfoActionBar" parent="@*android:style/ThemeOverlay.Material">
        <item name="android:actionOverflowButtonStyle">@*com.google.android.calendar:style/EventInfoActionBarOverflow</item>
    </style>
    <style name="EventInfoActionBarOverflow" parent="@*android:style/Widget.Material.ActionButton.Overflow">
        <item name="android:src">@*com.google.android.calendar:drawable/ic_overflow_white_34</item>
    </style>

    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">@*com.google.android.calendar:color/abc_hint_foreground_material_dark</item>
        <item name="android:textColorHint">@*com.google.android.calendar:color/abc_hint_foreground_material_light</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>


    <style name="ActionbarTheme.Compat" parent="@*com.google.android.calendar:style/ActionbarTheme">
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:displayOptions">homeAsUp|showTitle</item>
    </style>
    <style name="Theme.Actionbar.Base" parent="@*android:style/Widget.Material.ActionBar.Solid" />



</resources>
