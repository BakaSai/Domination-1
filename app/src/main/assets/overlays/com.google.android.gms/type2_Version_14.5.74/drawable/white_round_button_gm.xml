<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@*android:color/button_material_dark" />
            <stroke android:width="1.0dip" android:color="@*android:color/secondary_text_default_material_dark" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@*android:color/dim_foreground_light" />
            <stroke android:width="1.0dip" android:color="@*android:color/white" />
        </shape>
    </item>
</selector>
