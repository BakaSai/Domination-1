<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CardView" parent="@*com.google.android.gms:style/Base.CardView">
        <item name="*com.google.android.gms:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="IdentityWhiteAppTheme" parent="@*com.google.android.gms:style/IdentityWhiteAppThemeBase">
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.google.android.gms:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>

    <style name="udc.Theme.Fullscreen.WhiteStatusBar" parent="@*com.google.android.gms:style/udc.Theme.Fullscreen">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="TpActivityTheme" parent="@*com.google.android.gms:style/BaseTpActivityTheme">
        <item name="android:statusBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>



    <style name="common.Theme.Base.GoogleSettings" parent="@*com.google.android.gms:style/Theme.AppCompat">
        <item name="*com.google.android.gms:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="*com.google.android.gms:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
</resources>
